<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/ivline1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:src="@drawable/register_line" />
    <TextView
        android:id="@+id/tvnewhome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ivline"
        android:layout_margin="8dp"
        android:text="새 주소 입력 : "/>
    <EditText
        android:id="@+id/etnewhome"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tvpr"
        android:layout_margin="8dp"
        android:layout_toRightOf="@+id/tvpr"/>
</RelativeLayout>